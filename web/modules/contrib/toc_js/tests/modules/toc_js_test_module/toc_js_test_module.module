<?php

/**
 * @file
 * Contains hook implementations for the toc_js_test_module module.
 */

/**
 * Implements hook_preprocess_node().
 */
function toc_js_test_module_preprocess_node(&$variables) {
  if ($variables['node']->getType() === 'article') {
    $variables['attributes']['class'][] = 'node';
  }
}
